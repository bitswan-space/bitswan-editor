{
  "name": "bitswan-pre",
  "displayName": "bitswan-pre",
  "description": "Extension for interacting with Bitswan Pipeline Runtime Environment",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bitswan-pre",
          "title": "Bitswan PRE",
          "icon": "resources/bitswan-logo.svg"
        }
      ]
    },
    "views": {
      "bitswan-pre": [
        {
          "type": "webview",
          "id": "bitswanPRE",
          "name": ""
        }
      ]
    },
    "commands": [
      {
        "command": "extension.deployCurrentNotebook",
        "title": "Deploy Current Jupyter Notebook"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "jszip": "^3.10.1",
    "ws": "^8.18.0"
  }
}
